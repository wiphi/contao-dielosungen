{
	"name":"wiphi/contao-dielosungen",
	"description":"Importiere die Herrnhuter Losung in Contao und zeige sie im Frontend an.",
	"keywords":["contao","module","Herrnhuter Losung", "Die Losungen"],
	"type":"contao-bundle",
	"homepage":"https://github.com/wiphi/contao-dielosungen",
	"license":"MIT License",
	"authors":[
		{
			"name":"Philipp Winkel",
			"homepage":"https://github.com/wiphi/contao-dielosungen",
			"role":"Developer"
		}
	],
	"support":{
		"issues":"https://github.com/wiphi/contao-dielosungen/issues",
		"source":"https://github.com/wiphi/contao-dielosungen"
	},
	"require":{
		"php":"~7.0",
		"contao/core-bundle":"~4.4"
	},
	"autoload":{
		"psr-4": {
			"WiPhi\\DieLosungen\\": "src/"
		},
		"classmap": [
			"src/Resources/contao"
		],
		"exclude-from-classmap": [
			"src/Resources/contao/config",
			"src/Resources/contao/dca",
			"src/Resources/contao/languages",
			"src/Resources/contao/templates"
		]	
	},
	"extra":{
		"contao-manager-plugin": "WiPhi\\DieLosungen\\ContaoManager\\Plugin",

		"branch-alias": {
			"dev-contao4": "2.0.x-dev"
		}
	}
}
